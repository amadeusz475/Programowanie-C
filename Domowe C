//Domowe 17.03.2017

#include <stdio.h>

int main()
{
    unsigned int i,pom,suma=0,przeciecie=0;
    unsigned int zbior1=0,zbior2=0;

    scanf("%u", &pom);
    while(pom!=-1){
        zbior1 |= 1<<pom;
        scanf("%u", &pom);
    }

    scanf("%u", &pom);
    while(pom!=-1){
        zbior2 |= 1<<pom;
        scanf("%u", &pom);
    }

    przeciecie = zbior1 & zbior2;
    suma = zbior1 | zbior2;

    printf("\nsuma: ");
    for(i=0;i<=31;i++){
        if(((suma>>i)%2)==1){
            printf("%u ", i);
        }
    }
    printf("\nprzeciecie: ");
    for(i=0;i<=31;i++){
        if(((przeciecie>>i)%2)==1){
            printf("%u ", i);
        }
    }

    return 0;
}

//Domowe 24.03.2017

#include <stdio.h>

int main()
{
    char znak;
    int tab[10],i;

    for(i=0;i<10;i++){
        tab[i]=0;
    }

    while(scanf("%c", &znak)!=EOF){
        if(znak>47 && znak<58){
            tab[znak-48]++;
        }
        
    for(i=0;i<10;i++){
        printf("%d : %d\n",i, tab[i]);
    }

    return 0;
}

  //Domowe 31.03.2017

#include <stdio.h>
#include <string.h>

int palindrom (char *slowo)
{
    int i,l;
    l=strlen(slowo);
    for(i=0;i<l;i++){
        if(slowo[i]!=slowo[l-i-1]) return 0;
    }
    return 1;
}

int main()
{
    char tab[100];
    scanf("%s", &tab);

    if(palindrom(tab)==0) printf("NIE");
    else printf("TAK");

    return 0;
}

//Domowe 07.04.2017

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int liczba_zestawow,ile_elementow,suma=0,i,j,pom_sum=0;
    double srednia=0,pom_sr=0;
    int *zestawy;
    scanf("%d", &liczba_zestawow);

    for(i=0;i<liczba_zestawow;i++){
        scanf("%d", &ile_elementow);
        zestawy = malloc(sizeof(int)*ile_elementow);
        for(j=0;j<ile_elementow;j++){
            scanf("%d" , &zestawy[j]);
            pom_sum+=zestawy[j];
        }
        suma+=pom_sum;
        pom_sr=(double)pom_sum/(double)ile_elementow;
        if(pom_sr>srednia) srednia=pom_sr;
        pom_sum=0;
        pom_sr=0;
        free(zestawy);
    }
    printf("SUMA: %d\nSREDNIA: %f", suma,srednia );

    return 0;
}
