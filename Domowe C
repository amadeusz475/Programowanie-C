//Domowe 17.03.2017

#include <stdio.h>

int main()
{
    unsigned int i,pom,suma=0,przeciecie=0;
    unsigned int zbior1=0,zbior2=0;

    scanf("%u", &pom);
    while(pom!=-1){
        zbior1 |= 1<<pom;
        scanf("%u", &pom);
    }

    scanf("%u", &pom);
    while(pom!=-1){
        zbior2 |= 1<<pom;
        scanf("%u", &pom);
    }

    przeciecie = zbior1 & zbior2;
    suma = zbior1 | zbior2;

    printf("\nsuma: ");
    for(i=0;i<=31;i++){
        if(((suma>>i)%2)==1){
            printf("%u ", i);
        }
    }
    printf("\nprzeciecie: ");
    for(i=0;i<=31;i++){
        if(((przeciecie>>i)%2)==1){
            printf("%u ", i);
        }
    }

    return 0;
}

//Domowe 24.03.2017

#include <stdio.h>

int main()
{
    char znak;
    int tab[10],i;

    for(i=0;i<10;i++){
        tab[i]=0;
    }

    while(znak!=EOF){
        scanf("%c", &znak);
        if(znak>47 && znak<58){
            tab[znak-49]++;
        }
    }
    for(i=0;i<10;i++){
        printf("%d : %d\n",i, tab[i]);
    }

    return 0;
}

